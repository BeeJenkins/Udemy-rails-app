<div class = 'container'>

<%= simple_form_for(@template_one) do |f| %>
  <div class="form-inputs">
    <%= f.input :about_title, label:false %>
    <%= f.input :about_text, label:false %>
    <%= f.input :about_image, label:false, as: :file %>
    <%= f.input :about_show, label:false %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
</div>